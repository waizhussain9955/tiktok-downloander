{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/favicon.ico",
            "destination": "/static/img/favicon.ico"
        },
        {
            "source": "/sitemap.xml",
            "destination": "/sitemap.xml"
        },
        {
            "source": "/robots.txt",
            "destination": "/robots.txt"
        },
        {
            "source": "/blog.html",
            "destination": "/static/blog.html"
        },
        {
            "source": "/tiktok-video-downloader-(.*).html",
            "destination": "/static/tiktok-video-downloader-$1.html"
        },
        {
            "source": "/download-tiktok-videos-by-username.html",
            "destination": "/static/download-tiktok-videos-by-username.html"
        },
        {
            "source": "/(.*).html",
            "destination": "/static/$1.html"
        },
        {
            "source": "/",
            "destination": "/static/index.html"
        }
    ]
}